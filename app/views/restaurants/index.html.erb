  <h1>All Restaurants</h1>
  <ul>
    <% @restaurants.each do |restaurant| %>
    <li><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
    <br>
    <%= restaurant.address %></li>
    <br>
    <%= restaurant.phone_number %></li>
    <br>
    <%= restaurant.category %></li>
    <% end %>
  </ul>
